{
    "manifest_version":3,
    "name":"Wheather",
    "version":"0.1.0",
    "description":"Tells the wheather of current location",
    "action":{
        "default_popup":"popup.html",
        "default_icon": {
            
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    
    "permissions":["activeTab","geolocation"]
}